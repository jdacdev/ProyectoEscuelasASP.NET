@*
@model List<EscuelaBase>
*@
<table>
    <thead class="table table-dark table-hover">
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Nombre</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var objEscuelaBase in @Model)
    {
        <tr>
            <td>@objEscuelaBase.Id</td>
            <td>@objEscuelaBase.Nombre</td>
        </tr>
    }
    </tbody>
</table>